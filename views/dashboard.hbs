{{!-- <h1>RescueLife</h1> --}}
{{> header}}
<div class="contentContainer">
    {{!-- <div class="card-panel filterBar">
        <span>Filters</span>
        <span><Button>button</Button></span>
    </div> --}}


    <ul class="collapsible">
        <li>
            <div class="collapsible-header filtersPanel"><span>Filters</span></div>

            <div class="collapsible-body">
                <div class="row">
                    <div class="col s3">
                        <div class="card-panel dogs">
                            <div class="row">
                                <div class="col s6">
                                    <span>Dogs</span>
                                </div>
                                <div class="col s6">
                                    <div class="switch">
                                        <label>
                                            <input type="checkbox">
                                            <span class="lever"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col s3">
                        <div class="card-panel cats">
                            <div class="row">
                                <div class="col s6">
                                    <span>Cats</span>
                                </div>
                                <div class="col s6">
                                    <div class="switch">
                                        <label>
                                            <input type="checkbox">
                                            <span class="lever"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col s3">
                        <div class="card-panel equines">
                            <div class="row">
                                <div class="col s6">
                                    <span>Equines</span>
                                </div>
                                <div class="col s6">
                                    <div class="switch">
                                        <label>
                                            <input type="checkbox">
                                            <span class="lever"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col s3">
                        <div class="card-panel others">
                            <div class="row">
                                <div class="col s6">
                                    <span>Others</span>
                                </div>
                                <div class="col s6">
                                    <div class="switch">
                                        <label>
                                            <input type="checkbox">
                                            <span class="lever"></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>



</div>
</li>
</ul>

</div>

<div class="container">
    {{> petsList}}
</div>














<div class="container">
    <div class="row">
        <div class="col s6">
            <h1>Contact Manager</h1>
        </div>
        <div class="col s6">
            <p class="right-align"> Hello, {{username}} </p>
        </div>
    </div>
    <div class="row">
        <div class="col s12">
            {{> contactsForm}}
        </div>
    </div>
    <div class="divider"></div>
    <div class="row">
        {{> contactsList}}
    </div>
</div>